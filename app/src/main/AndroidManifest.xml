<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creativeclan.test2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="S A M"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisteratioActivity" />
        <activity android:name=".UserAreaActivity" />
        <activity android:name=".StockOptions" />
        <activity android:name=".MainActivity" />
        <activity android:name=".UserAreaActivity2" />
        <activity android:name=".GetData" />
        <activity android:name=".Display" />
        <activity android:name=".DecoderActivity" />
        <activity android:name=".Serial" />

        <service
            android:name=".UsbService"
            android:enabled="true" />

        <activity android:name=".Update" />
        <activity android:name=".UserAreaActivity3"></activity>
    </application>

</manifest>